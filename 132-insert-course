public Id insertCourse(String name, String details, Integer credits) {
    // code here
    apxio__Course__c cs = new apxio__Course__c();
    cs.Name=name;
    cs.apxio__Course_Details__c=details;
    String cred = String.valueOf(credits);
    List<String> credValues = new List<String>{'1','2','3','4'};
    if(!credValues.contains(cred))
    {
        return null;
    }  
    cs.apxio__Credits__c = cred;
    INSERT cs;
    return cs.Id;
}